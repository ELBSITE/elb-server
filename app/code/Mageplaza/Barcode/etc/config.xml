<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <mpbarcode>
            <general>
                <enabled>1</enabled>
                <attribute_barcode>mp_barcode</attribute_barcode>
                <barcode_template>[10N]</barcode_template>
                <btn_generate>1</btn_generate>
            </general>
            <barcode>
                <type>C93</type>
                <paper>
                    <paper_template>OL161</paper_template>
                    <paper_size>Letter</paper_size>
                    <width>95.25</width>
                    <height>31.75</height>
                    <padding>0</padding>
                    <margin>29.4513;9.525;29.4513;9.525</margin>
                    <vertical>5.99948</vertical>
                    <horizontal>6.35</horizontal>
                    <orientation>P</orientation>
                </paper>
                <label>
                    <template>standard</template>
                    <width>1</width>
                    <height>1</height>
                    <size>1</size>
                </label>
            </barcode>
        </mpbarcode>
    </default>
</config>
