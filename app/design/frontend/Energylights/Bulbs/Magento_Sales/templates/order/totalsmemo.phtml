<?php foreach ($block->getTotals() as $_code => $_total) : ?>
 <?php   if ($_total->getLabel() == "Grand Total (Excl.Tax)") {  
                   continue; 
         } ?> 
    <?php if ($_total->getBlockName()) : ?>
        <?= $block->getChildHtml($_total->getBlockName(), false) ?>
    <?php else :?>
    <tr class="<?= $block->escapeHtmlAttr($_code) ?>">
       <?php  if ($_total->getLabel() == "Grand Total (Incl.Tax)") { ?>
                 <th  <?= /* @noEscape */ $block->getLabelProperties() ?> scope="row" style="text-align:right; padding-top:10px; padding-right:20px">
                  <?php echo "Total Test";?>
                  </th>
                  
         <?php }else{ ?> 
         
        <th  <?= /* @noEscape */ $block->getLabelProperties() ?> scope="row" style="text-align:right; padding-top:10px; padding-right:20px">
            <?php if ($_total->getStrong()) : ?>
           
                <strong><?= $block->escapeHtml($_total->getLabel()) ?></strong>
            <?php else : ?>
                <?= $block->escapeHtml($_total->getLabel()) ?>
            <?php endif ?>
        </th>
        
        <?php } ?>
        
        <td <?= /* @noEscape */ $block->getValueProperties() ?> data-th="<?= $block->escapeHtmlAttr($_total->getLabel()) ?>" style="text-align:right; padding-top:10px; padding-right: 10px;">
            <?php if ($_total->getStrong()) : ?>
                <strong><?= /* @noEscape */ $block->formatValue($_total) ?></strong>
            <?php else : ?>
                <?= /* @noEscape */ $block->formatValue($_total) ?>
            <?php endif?>
        </td>
    </tr>
    <?php endif; ?>
